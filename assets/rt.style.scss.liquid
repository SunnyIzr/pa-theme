/* CARTS */
.cart-page{
  .empty{
    margin-bottom: 30px;
  }
  .cart-item{
    float: left;
    width: 100%;
    margin-bottom: 30px;
    border-bottom: 1px dashed {{settings.b_tables_border}};
    padding-bottom: 30px;
  }
  .cart-items{
    display: inline-block;
    width: 100%;

    .subtotal{
      font-size: {{settings.product_price_font_size | plus:0 | append:'px'}};
      color: {{settings.product_price_colour}};
      font-weight: {{settings.product_price_font_weight}};
    }
    .cart-left{
      float: left;
      width: 160px;
      text-align: center;
      position: relative;

      .cart-remove{
        position: absolute;
        top: 50%;
        left: 50%;
        margin: -20px 0 0 -20px;
        z-index: 2;
        color: {{settings.b_scaffolding_bg}};
        width: 40px;
        height: 40px;
        text-align: center;
        vertical-align: middle;
        cursor: pointer;
        opacity: 0.8;
        filter: alpha(opacity=80);
        -webkit-transition: 0.2s all ease;
        -moz-transition: 0.2s all ease;
        -o-transition: 0.2s all ease;
        -ms-transition: 0.2s all ease;
        transition: 0.2s all ease;

        i{
          line-height: 40px;
        }

        &:hover{
          opacity: 1;
          filter: alpha(opacity=100);
        }

        &:after{
          position: absolute;
          z-index: -1;
          content: '';
          background: {{settings.b_scaffolding_color}};
          width: 100%;
          height: 100%;
          top: 0;
          left: 0;
          opacity: 0.8;
          filter: alpha(opacity=80);
          -webkit-border-radius: 50% !important;
          -moz-border-radius: 50% !important;
          border-radius: 50% !important;
        }
      }
    }
    .cart-right{
      float: left;
      width: -webkit-calc(100% - 160px);
      width: calc(100% - 160px);
      padding-left: 30px;
      text-align: left;

      .product-title{
        font-size: {{settings.wrapper_title_size | plus:0 | append:'px'}};
      }
      .product-price{
        color: {{settings.product_price_colour}};
        font-size: {{settings.wrapper_price_size | plus:0 | append:'px'}};
        font-weight: {{settings.product_price_font_weight}};
        margin-top: 6px;
      }

      .qty{
        display: inline-block;
        font-size: 0;
        margin: 25px 0 0;
        border: 1px solid {{settings.b_tables_border}};

        .button{
          background: none;
          line-height: 30px;
          cursor: pointer;
          float: left;
          font-size: {{settings.general_font_size | plus:0 | append:'px'}};
          padding: 0 10px;
          -webkit-transition: all 0.2s ease;
          -moz-transition: all 0.2s ease;
          -o-transition: all 0.2s ease;
          -ms-transition: all 0.2s ease;
          transition: all 0.2s ease;
          text-align: center;
          vertical-align: middle;
          width: 35px;

          &.icomoon-plus{
            border-left: 1px solid {{settings.b_tables_border}};
          }
          &.icomoon-minus{
            border-right: 1px solid {{settings.b_tables_border}};
          }
        }
        .quantity{
          line-height: 30px;
          height: 30px!important;
          max-width: 100%;
          height: auto;
          margin: 0;
          float: left;
          vertical-align: middle;
          text-align: center;
          border: none;
          width: 70px;
        }
      }
    }
  }
  #checkout-addnote{
    margin-top: 25px;
  }
  #wrapper-response{
    display: none;
    margin-top: 20px;
  }
  #update-proceed{
    overflow: visible;
    margin-bottom: 15px;
  }
  #checkout-proceed{overflow: visible;}
}
.cart-menu-icon{
  position: relative;

  em{
    position: absolute;
    background-color: {{settings.header_colour}};
    width: 15px;
    height: 15px;
    overflow: hidden;
    line-height: 15px;
    color: {{settings.header_bg}};
    text-align: center;
    vertical-align: middle;
    top: {{settings.header_height | divided_by:2 | minus:15 | append:'px'}};
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
