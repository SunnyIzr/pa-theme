  }
  .message{
    text-align: center;
    font-size: 1.5em;
    line-height: 1.5em;
    margin: 0 auto;
  }

  .widget-wrapper{
    overflow: visible!important;
  }

  .title{
    margin-top: 15px;
    margin-bottom: 35px;
  }

  ul{
    margin-bottom: 0;
  }

  ul li {
    position: relative;
    border-bottom: 1px dashed {{settings.b_tables_border}};
    padding-bottom: 19px;
    margin-bottom: 20px;
    padding-top: 0;

    &:last-child{
      border-bottom: 0;
      margin-bottom: 0;
    }

    figure {
      float: left;
      width: 40px;
      margin-right: 20px;
    }

    .remove {
      position: absolute;
      top: 0;
      right: -15px;
    }

    .list_content {
      padding-left: 60px;
    }

    .quantity{
      line-height: normal;

      .amount{
        font-size: {{settings.wrapper_price_size | plus:0 | append:'px'}};
        color: {{settings.product_price_colour}};
        font-weight: {{settings.product_price_font_weight}};
      }
    }

    h5{
      line-height: normal;
      font-weight: normal;
      font-size: {{settings.wrapper_title_size | plus:0 | append:'px'}};
    }
  }

  .cart-empty span{
    width: 68px;
    height: 100px;
    background-image: url({{settings.cart_icon_empty | asset_url}});
    background-repeat: no-repeat;
    background-position: left top;
    background-size: 68px 100px;
    display: inline-block;
  }

  .subtotal {
    width: 100%;
    padding: 22px 0;
    border-top: 1px dashed {{settings.b_tables_border}};
    font-size: 20px;
    font-weight: 500;

    .title{
      margin: 0;
      display: table-cell;
      vertical-align: middle;

      &:after{
        display: none;
      }
    }

    .amount {
      display: table-cell;
      vertical-align: middle;
      width: 100%;
      text-align: right;
      font-size: {{settings.wrapper_price_size | plus:0 | append:'px'}};
      color: {{settings.product_price_colour}};
      font-weight: {{settings.product_price_font_weight}};
    }
  }

  .buttons{
    width: 100%;
    padding: 30px 0;
    border-top: 1px dashed {{settings.b_tables_border}};

    .btn-1{
      margin-top: 15px!important;
    }
  }
}
