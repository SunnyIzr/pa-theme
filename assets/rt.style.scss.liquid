.group-input {
  position: relative;
  margin: 30px 0 24px;

  input, .input {
    color: inherit;
    padding: 10px!important;
    display: block;
    width: 100%;
    border: none;
    border-bottom: 1px solid {{settings.footer_txt_colour}};
    background: transparent;
    margin-bottom: 0;
    height: 36px;

    &:-moz-placeholder{
      color: {{settings.footer_txt_colour}};
    }
    &::-moz-placeholder{
      color: {{settings.footer_txt_colour}};
    }
    &:-ms-input-placeholder{
      color: {{settings.footer_txt_colour}};
    }
    &::-webkit-input-placeholder{
      color: {{settings.footer_txt_colour}};
    }
  }
  input:focus, .input:focus {
    outline: none;
  }
  button{
    border: 0;
    background: none;
    position: absolute;
    right: 5px;
    top: 10px;
    outline: none;
    color: {{settings.footer_txt_colour}};
    padding-left: 0;
    padding-right: 0;
  }
  .bar {
    position: relative;
    display: block;
    width: 100%;
  }
  .bar:before,
  .bar:after {
    content: '';
    height: 2px;
    width: 0;
    bottom: 0;
    position: absolute;
    background: {{settings.footer_txt_colour}};
    transition: width 0.2s ease-out;
    -moz-transition: width 0.2s ease-out;
    -webkit-transition: width 0.2s ease-out;
  }
  .bar:before {
    left: 50%;
  }
  .bar:after {
    right: 50%;
  }
  .highlight {
    position: absolute;
    height: 60%;
    width: 100px;
    top: 25%;
    left: 0;
    pointer-events: none;
    opacity: 0.5;
    display: none;
  }
}
