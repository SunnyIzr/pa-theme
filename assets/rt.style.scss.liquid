/* ================ HEADER ==================== */
#arrow-nav-row {
  background-color:#fff;
  height:25px;
}

#header{
  position: relative;
  top: 0!important;
  z-index: 10;
  background: transparent;

  &:before{
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: block;
    background: {{settings.header_bg}};
  }

  &.sticky{
    #site-navigation-wrap{
      -webkit-transition: all 0.75s ease-in-out;
      -moz-transition: all 0.75s ease-in-out;
      -o-transition: all 0.75s ease-in-out;
      -ms-transition: all 0.75s ease-in-out;
      transition: all 0.75s ease-in-out;
    }
  }

  &.on{
    position: fixed!important;
    z-index: 1000;
    width: 100%;
    top: 0;
    left: 0;
    -webkit-box-shadow: 0px 2px 5px 0px rgba(30, 30, 30, 0.2);
    -moz-box-shadow: 0px 2px 5px 0px rgba(30, 30, 30, 0.2);
    box-shadow: 0px 2px 5px 0px rgba(30, 30, 30, 0.2);

    &:before{
      opacity: 0.88!important;
      filter: alpha(opacity=88)!important;
    }
    &:hover:before{
      opacity: 1!important;
      filter: alpha(opacity=100)!important;
    }

    &.sticky{
      .pc_navbar .pc_nav>li>a>span,
      #site-logo,
      #site-navigation-wrap,
      .pc_navbar .pc_nav>li{
        height: 60px;
        line-height: 60px;
      }
    }
  }

  .navigation_wrapper{
    position:relative;
    display: table;
    width: 100%;
  }
  #site-logo{
    display: table-cell;
    vertical-align: middle;
    height: 88px;
    line-height: 88px;
  }
  #site-navigation-wrap{
    height: 88px;
    line-height: 88px;
    display: table-cell;
    vertical-align: middle;
  }

  .search-menu-icon{
    &.active{
      .search-table-wraper{
        display:block;
      }
    }
  }

  &.is_search{
    .search-table-wraper{
      height: 40px;
    }
  }
  .search-table-wraper{
    position: relative;
    height: 0;
    overflow: hidden;
    -webkit-transition: all .2s ease-out;
    -moz-transition: all .2s ease-out;
    -ms-transition: all .2s ease-out;
    -o-transition: all .2s ease-out;
    transition: all .2s ease-out;

    &:before{
      content: '';
      width: 100%;
      height: 100%;
      position: absolute;
      background-color: {{settings.search_bg}};
    }

    .search-form{
      margin: 5px 0;
      position: relative;
    }

    .search-table{
      display: table;
      width: 100%;

      .search-field {
        display: table-cell;
        vertical-align: middle;

        input{
          width: 100%;
          border: 0;
          margin:0;
          padding:0 5px!important;
          background: transparent;
          color: {{settings.search_color}};
          font-size: 95%;
          font-weight: 300;

          &:-moz-placeholder{
            color: {{settings.search_color}};
          }
          &::-moz-placeholder{
            color: {{settings.search_color}};
          }
          &:-ms-input-placeholder{
            color: {{settings.search_color}};
          }
          &::-webkit-input-placeholder{
            color: {{settings.search_color}};
          }
        }
      }

      .search-button {
        display: table-cell;
        width: 20px;
        vertical-align: middle;
        text-align: center;

        .search-submit2{
          vertical-align: middle;
          background-color: transparent;
          border: none;
          outline: none;
          height: 30px;
          line-height: 30px;
          margin: 0;
          padding: 0;
          width: 20px;
          text-indent: 0;
          cursor: pointer;
          text-shadow: none;

          i{
            color: {{settings.search_color}};
            font-size: 95%;
          }
        }
      }
    }
  }
  .customer-menu-icon{
    &:hover{
      .dropdown-menu{display:block;}
    }
  }
}
