/* ================= begin BOOTSTRAP ================= */
/*
** Base CSS
**/
/* begin Headings, body, etc */
html {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
html,
html body{
  line-height: 20px;
}
body {
  margin: 0;
  font-family: {{general_font}};
  font-size: {{settings.general_font_size | plus:0 | append:'px'}};
  font-weight: {{settings.general_font_style}};
  line-height: 20px;
  color: {{settings.b_scaffolding_color}};
  background-color: {{settings.b_scaffolding_bg}};
}
iframe{
  max-width: 100%;
}
label,
input,
button,
select,
textarea{
  font-size: {{settings.general_font_size | plus:0 | append:'px'}};
}
a {
  color: {{settings.b_links_colour}};
}

a:focus {
  color: {{settings.b_links_colour_hover}};
}
.text-error {
  color: {{settings.b_states_alerts_error_txt}};
}
a.text-error:hover,
a.text-error:focus {
  color: {{settings.b_states_alerts_error_bg}};
}
.text-success {
  color: {{settings.b_states_alerts_success_txt}};
}
a.text-success:hover,
a.text-success:focus {
  color: {{settings.b_states_alerts_success_bg}};
}
h1 small,
h2 small,
h3 small,
h4 small,
h5 small,
h6 small {
  color: {{settings.b_scaffolding_color}};
}
blockquote, .blockquote{
  font-size: {{settings.general_font_size | plus:0 | append:'px'}};
  border-left-width: 3px;
}
blockquote small {
  color: {{settings.b_scaffolding_color}};
}
.label-success,
.badge-success {
  background-color: {{settings.b_states_alerts_error_txt}};
}
.label-success[href],
.badge-success[href] {
  background-color: {{settings.b_states_alerts_success_bg}};
}
.alert-success {
  background-color: {{settings.b_states_alerts_success_bg}};
  border-color: {{settings.b_states_alerts_success_bg}};
  color: {{settings.b_states_alerts_success_txt}};
}
.alert-error{
  background-color: {{settings.b_states_alerts_error_bg}};
  border-color: {{settings.b_states_alerts_error_bg}};
  color: {{settings.b_states_alerts_error_txt}};
}
.alert{
  padding: 10px;
}
.alert ul{
  margin-bottom: 0;
  padding-left: 20px;
}
h1, .h1{
  font-size: {{settings.b_heading_h1 | plus: 0 | append: 'px'}};
}
h2, .h2{
  font-size: {{settings.b_heading_h2 | plus: 0 | append: 'px'}};
}
h3, .h3{
  font-size: {{settings.b_heading_h3 | plus: 0 | append: 'px'}};
}
h4, .h4{
  font-size: {{settings.b_heading_h4 | plus: 0 | append: 'px'}};
}
h5, .h5{
  font-size: {{settings.b_heading_h5 | plus: 0 | append: 'px'}};
}
h6, .h6{
  font-size: {{settings.b_heading_h6 | plus: 0 | append: 'px'}};
}
/* end Headings, body, etc */

/* begin Tables */
table {
  background-color: {{settings.b_tables_bg}};
}
.table th,
.table td {
  border-top: 1px solid {{settings.b_tables_border}};
}
.table tbody + tbody {
  border-top: 2px solid {{settings.b_tables_border}};
}
.table .table {
  background-color: {{settings.b_tables_bg}};
}
.table-bordered {
  border: 1px solid {{settings.b_tables_border}};
}
.table-bordered th,
.table-bordered td {
  border-left: 1px solid {{settings.b_tables_border}};
}
.table-striped tbody > tr:nth-child(odd) > td,
.table-striped tbody > tr:nth-child(odd) > th {
  background-color: {{settings.b_tables_bg_accent}};
}
.table-hover tbody tr:hover > td,
.table-hover tbody tr:hover > th {
  background-color: {{settings.b_tables_bg_hover}};
}
.table tbody tr.success > td {
  background-color: {{settings.b_states_alerts_success_bg}};
}
.table tbody tr.error > td {
  background-color: {{settings.b_states_alerts_error_bg}};
}
.table-hover tbody tr.success:hover > td {
  background-color: {{settings.b_states_alerts_success_txt}};
}
.table-hover tbody tr.error:hover > td {
  background-color: {{settings.b_states_alerts_error_txt}};
}
/* end Tables */

/* begin Forms */
textarea,
input[type="text"],
input[type="password"],
input[type="datetime"],
input[type="datetime-local"],
input[type="date"],
input[type="month"],
input[type="time"],
input[type="week"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"],
.uneditable-input {
  background-color: {{settings.b_forms_input_bg}};
  border: 1px solid {{settings.b_forms_input_border}};
  margin-bottom: 15px;
}
select {
  border: 1px solid {{settings.b_forms_input_border}};
  background-color: {{settings.b_forms_input_bg}};
  padding: 0 12px!important;
  margin-bottom: 15px;
}
input:-moz-placeholder,
textarea:-moz-placeholder {
  color: {{settings.b_forms_phd_txt}};
}
input::-moz-placeholder,
textarea::-moz-placeholder {
  color: {{settings.b_forms_phd_txt}};
}
input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
  color: {{settings.b_forms_phd_txt}};
}
input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
  color: {{settings.b_forms_phd_txt}};
}
.control-group.error .control-label,
.control-group.error .help-block,
.control-group.error .help-inline {
  color: {{settings.b_states_alerts_error_txt}};
}
.control-group.error .checkbox,
.control-group.error .radio,
.control-group.error input,
.control-group.error select,
.control-group.error textarea {
  color: {{settings.b_states_alerts_error_txt}};
}
.control-group.error input,
.control-group.error select,
.control-group.error textarea {
  border-color: {{settings.b_states_alerts_error_txt}};
}
.control-group.error input:focus,
.control-group.error select:focus,
.control-group.error textarea:focus {
  border-color: {{settings.b_states_alerts_error_bg}};
}
.control-group.error .input-prepend .add-on,
.control-group.error .input-append .add-on {
  color: {{settings.b_states_alerts_error_txt}};
  background-color: {{settings.b_states_alerts_error_bg}};
  border-color: {{settings.b_states_alerts_error_txt}};
}
.control-group.success .control-label,
.control-group.success .help-block,
.control-group.success .help-inline {
  color: {{settings.b_states_alerts_success_txt}};
}
.control-group.success .checkbox,
.control-group.success .radio,
.control-group.success input,
.control-group.success select,
.control-group.success textarea {
  color: {{settings.b_states_alerts_success_txt}};
}
.control-group.success input,
.control-group.success select,
.control-group.success textarea {
  border-color: {{settings.b_states_alerts_success_txt}};
}
.control-group.success input:focus,
.control-group.success select:focus,
.control-group.success textarea:focus {
  border-color: {{settings.b_states_alerts_success_bg}};
}
.control-group.success .input-prepend .add-on,
.control-group.success .input-append .add-on {
  color: {{settings.b_states_alerts_success_txt}};
  background-color: {{settings.b_states_alerts_success_bg}};
  border-color: {{settings.b_states_alerts_success_txt}};
}
/* end Forms */
/*
** end Base CSS
**/

/*
** begin Plugins
**/
/* begin Dropdown */
.dropdown-menu {
  background-color: {{settings.b_scaffolding_bg}};
  border: 1px solid {{settings.b_tables_border}};
  color: {{settings.b_scaffolding_color}};
  text-align: left;
}
.dropdown-menu:before,
.dropdown-menu:after{
  display: none!important;
}
.dropdown-parent-submenu .dropdown-menu{
  left: 100%!important;
  top: 0;
  padding: 5px 15px;
  margin-top: -1px;
  min-width:200px;
}
.dropdown-parent-submenu .dropdown-menu:before{
  content: '';
  display: inline-block;
  border-left: 0;
  border-top: 7px solid transparent;
  border-bottom: 7px solid transparent;
  border-right: 7px solid {{settings.b_tables_border}};
  position: absolute;
  left: -7px;
  top: 9px;
}
.dropdown-parent-submenu .dropdown-menu:after{
  content: '';
  display: inline-block;
  border-left: 0;
  border-top: 6px solid transparent;
  border-bottom: 6px solid transparent;
  border-right: 6px solid {{settings.b_scaffolding_bg}};
  position: absolute;
  left: -6px;
  top: 10px;
}

.dropdown,
.dropdown-toggle{
  position: relative;
  line-height: 2em;
}
.dropdown .sub-dropdown,
.dropdown-toggle .sub-dropdown,
.dropdown .sub-dropdown1,
.dropdown-toggle .sub-dropdown1{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}
.dropdown .sub-dropdown,
.dropdown-toggle .sub-dropdown{
  padding-bottom: 20px;
  z-index: 1;
  cursor: default;
}
.dropdown .sub-dropdown1,
.dropdown-toggle .sub-dropdown1{
  z-index: 2;
  cursor: pointer;
}
.btn-group.open .dropdown-toggle{
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.open>.dropdown-menu{
  display: block!important;
}
/* end Dropdown */

/* begin Tooltips*/
.tooltip-inner {
  color: {{settings.b_tooltip_colour}};
  background-color: {{settings.b_tooltip_bg}};
  font-size: {{settings.b_tooltip_font_size | plus:0 | append:'px'}};
  padding: 6px;
}
.tooltip.top .tooltip-arrow {
  border-top-color: {{settings.b_tooltip_bg}};
  top: auto!important;
}
.tooltip.right .tooltip-arrow {
  border-right-color: {{settings.b_tooltip_bg}};
}
.tooltip.left .tooltip-arrow {
  border-left-color: {{settings.b_tooltip_bg}};
}
.tooltip.bottom .tooltip-arrow {
  border-bottom-color: {{settings.b_tooltip_bg}};
}
/* end Tooltips*/
/*
** end Plugins
**/

/* begin MISC */
.modal-backdrop, .modal-backdrop.fade.in{
  background: {{settings.b_scaffolding_color}};
}
/* end MISC */
/* ================= end BOOTSTRAP ================= */

/*
** HTML, Body settings
*/
{% case settings.site_bg %}
{% when 'bg_colour' %}
html{ background-color: {{ settings.bg_colour }} }

{% when 'bg_image' %}
html{ background-image: url({{ 'bg_image.png' | asset_url }} ); }

{% when 'bg_both' %}
html{
  background-color: {{ settings.bg_colour }};
  background-image: url({{ 'bg_image.png' | asset_url }} );
}
{% endcase %}

/***
Reset and overrides
***/
/* general body settings */
body {
  padding: 0px;
  margin: 0px;
  direction: ltr;
}
/* overides metro scrolbars in IE10 and makes them look like a default scrollbar */
@-ms-viewport{
  width: auto !important;
}
h1, h2, h3, h4, h5, h6 {
  font-family: {{header_font}};
}
button, input, select, textarea{
  font-size: {{settings.general_font_size | plus:0 | append:'px'}};
  font-family: {{general_font}};
}
/***
Fix link outlines after click
***/
a,a:focus, a:hover, a:active {
  outline: 0;
  text-decoration: none;
}
div, input, select, textarea, span, img, table, td, th, p, a, button, ul, li {
  -webkit-border-radius: 0 !important;
  -moz-border-radius: 0 !important;
  border-radius: 0 !important;
}
label{
  cursor: default;
  font-weight: normal;
  display: block;
}
ol li,
ul li{
  padding: 5px 0;
}
dl dd{
  margin-bottom: 20px;
  margin-left: 0;
}
table{
  width: 100%;
  background: {{settings.b_tables_bg}};
}
table thead,
table thead th,
table thead td{
  text-align: left;
  font-weight: normal;
  padding: 8px 20px;
